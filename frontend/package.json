    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-unsanitized": "^4.1.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
